{
	"name": "planet-escape",
	"version": "0.0.0",
	"description": "Planet escape! a game like no other",
	"scripts": {
		"init": "npm install && node node_modules/webpack-configuration/setup.js",
		"start": "node node_modules/webpack-configuration/webpack/server.js --env=dev",
		"build": "npm run test && npm run build:docs && npm run build:clean && npm run build:dist && npm run build:liveExample",
		"build:dist": "webpack --config=node_modules/webpack-configuration/webpack/webpack.server.conf.js --env=dist && npm run cssnano",
		"build:docs": "rimraf ./docs/** && jsdoc -c ./jsdocs.conf.json -u ./tutorials -t ./node_modules/ink-docstrap/template -R readme.md -r ./src -d docs",
		"build:clean": "rimraf ./dist/**",
		"build:liveExample": "webpack --config=node_modules/webpack-configuration/webpack/webpack.server.conf.js --env=liveExample",
		"analyze": "webpack --config=node_modules/webpack-configuration/webpack/webpack.server.conf.js --env=dist --profile --json | webpack-bundle-size-analyzer > stats.txt",
		"test": "npm run lint && jest --config=jestConfig.json --coverage --no-cache",
		"tdd": "npm run lint && jest --config=jestConfig.json --watch --no-cache",
		"releasePatch": "release-it",
		"releaseMinor": "release-it minor",
		"releaseMajor": "release-it major",
		"karma": "npm run lint && karma start",
		"karma:watch": "npm run lint && karma start --autoWatch=true --singleRun=false",
		"build:dll": "webpack --config=node_modules/webpack-configuration/webpack/webpack.server.conf.js --env=dll",
		"cssnano": "if test -e ./dist/bundle.css; then cssnano ./dist/bundle.css ./dist/bundle.css; else echo 'no css file to minify'; fi",
		"lint": "eslint -c=./.eslint ./src __tests__ __karma__ mocks"
	},
	"devDependencies": {
		"webpack-configuration": "git@bitbucket.org:patrickprivate/react-webpack-configuration.git#9.0.3"
	},
	"main": "dist",
	"dependencies": {
		"bootstrap": "^3.3.7",
		"canvas-prebuilt": "^1.6.5-prerelease.1"
	}
}